#include <iostream>
using namespace std;

int main(){
    int n,l,h,w;
    cin >> l>>n;
    for(int i=0;i<n;i++){
        cin >>w>>h;
        if(h<l || w<l){
            cout<< "UPLOAD ANOTHER"<< endl;
        }
        else if (h != w){
            cout<< "CROP IT"<< endl;
        }
        else{
            cout << "ACCEPTED"<<endl;
        }
    }
    return 0;
    
}
